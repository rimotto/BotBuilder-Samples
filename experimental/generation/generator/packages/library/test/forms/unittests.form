{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Quantity": {
            "type": "number",
            "minimum": 1,
            "maximum": 10
        },
        "Length": {
            "type": "integer"
        },
        "Complete": {
            "type": "string",
            "$entities": [
                "boolean"
            ]
        },
        "NameArray": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "$entities": [
                "personName"
            ]
        },       
        "EnumFruit": {
            "type": "string",
            "enum": [
                "orange",
                "apple",
                "pear",
                "banana"
            ]
        },
        "EnumArrayColor": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "yellow",
                    "red",
                    "blue",
                    "black",
                    "white"
                ]
            }
        },
        "Email": {
            "type": "string",
            "format": "email"
        },
        "URL1": {
            "type": "string",
            "format": "uri"
        },
        "URL2": {
            "type": "string",
            "format": "iri"
        },
        "DateTimeProperty": {
            "type": "string",
            "format": "date-time"
        },
        "DateProperty": {
            "type": "string",
            "format": "date"
        },
        "TimeProperty": {
            "type": "string",
            "format": "time"
        },
        "Name": {
            "type": "string",
            "$entities": [
                "personName"
            ],
            "pattern" : "^[a-z]{1,3}$"
        },
        "PhoneNumber": {
            "type": "string",
            "$entities": [
                "phonenumber"
            ]
        },
        "KeyphraseProperty": {
            "type": "string",
            "$entities": [
                "keyPhrase"
            ]
        },
        "Percentage": {
            "type": "string",
            "$entities": [
                "percentage"
            ]
        },
        "Age": {
            "$ref": "template:age.schema"
        },
        "OrdinalV2": {
            "$ref": "template:ordinal.schema"
        },
        "GeographyV2": {
            "$ref": "template:geography.schema"
        },
        "Money": {
            "$ref": "template:money.schema"
        },
        "Temperature": {
            "$ref": "template:temperature.schema"
        },
        "Dimension": {
            "$ref": "template:dimension.schema",
            "$units": "feet"
        },
        "DateTime": {
            "$ref": "template:datetime.schema"
        }
    },
    "required": [
        "Quantity",
        "Length",
        "Complete",
        "NameArray",
        "EnumFruit",
        "EnumArrayColor",
        "Email",
        "URL1",
        "URL2",
        "DateTimeProperty",
        "DateProperty",
        "TimeProperty",
        "Name",
        "PhoneNumber",
        "KeyphraseProperty",
        "Percentage",
        "Age",
        "OrdinalV2",
        "GeographyV2",
        "Money",
        "Temperature",
        "Dimension",
        "DateTime"
    ]
}