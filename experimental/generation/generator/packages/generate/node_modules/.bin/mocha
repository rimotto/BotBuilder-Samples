_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules/mocha/bin/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules/mocha/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/mocha" "$@"
else 
  exec node  "$basedir/../mocha/bin/mocha" "$@"
fi
