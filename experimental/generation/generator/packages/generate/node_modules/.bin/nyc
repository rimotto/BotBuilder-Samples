#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/registry.npmjs.org/nyc/14.1.1/node_modules/nyc/bin/nyc.js/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/registry.npmjs.org/nyc/14.1.1/node_modules/nyc/bin/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/registry.npmjs.org/nyc/14.1.1/node_modules/nyc/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/registry.npmjs.org/nyc/14.1.1/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/registry.npmjs.org/nyc/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules/.pnpm/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/temp/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/common/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/node_modules:/mnt/c/Users/chrim/source/repos/node_modules:/mnt/c/Users/chrim/source/node_modules:/mnt/c/Users/chrim/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules/nyc/bin/nyc.js/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules/nyc/bin/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules/nyc/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/generate/node_modules:/mnt/c/Users/chrim/source/repos/BotBuilder-Samples/experimental/generation/generator/packages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nyc/bin/nyc.js" "$@"
else 
  exec node  "$basedir/../nyc/bin/nyc.js" "$@"
fi
