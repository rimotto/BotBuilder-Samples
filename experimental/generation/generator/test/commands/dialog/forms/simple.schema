{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "age": {
            "type": "number"
        },
        "Meat": {
            "type": "string",
            "enum": [
                "bacon",
                "chicken",
                "ham",
                "pulled pork",
                "roast beef",
                "salami",
                "turkey",
                "none"
            ]
        }
    },
    "required": [
        "age"
    ]
}