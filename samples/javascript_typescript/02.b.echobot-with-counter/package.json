{
  "name": "echobot-with-counter-ts",
  "version": "1.0.0",
  "description": "Bot Builder v4 echo bot with counter sample",
  "main": "./lib/index.js",
  "scripts": {
    "build": "node_modules/typescript/bin/tsc --build",
    "start": "node_modules/typescript/bin/tsc --build && node ./lib/index.js",
    "watch": "concurrently --kill-others \"node_modules/typescript/bin/tsc -w\" \"nodemon ./lib/index.js\"",
    "lint": "node_modules/tslint/bin/tslint -c tslint.json 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "botbuilder": "^4.1.5",
    "botframework-config": "^4.1.5",
    "dotenv": "^6.1.0",
    "restify": "^7.2.3"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.0",
    "@types/restify": "7.2.6",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
